{
  "name": "Windows 98",
  "type": "light",
  "$schema": "vscode://schemas/color-theme",
  "colors": {
    "activityBar.background": "#DDDDDD",
    "activityBar.border": "#808080",
    "activityBar.foreground": "#000000",
    "activityBarBadge.background": "#000080",
    "activityBarBadge.foreground": "#FFFFFF",
    "button.background": "#DDDDDD",
    "button.foreground": "#000000",
    "button.hoverBackground": "#FFFFFF",
    "button.border": "#808080",
    "dropdown.background": "#DDDDDD",
    "dropdown.border": "#808080",
    "dropdown.foreground": "#000000",
    "dropdown.listBackground": "#FFFFFF",
    "editor.background": "#FFFFFF",
    "editor.foreground": "#000000",
    "editor.findMatchBackground": "#000080",
    "editor.findMatchHighlightBackground": "#0000801A",
    "editor.lineHighlightBackground": "#0000801A",
    "editor.selectionBackground": "#00008033",
    "editor.selectionForeground": "#FFFFFF",
    "editorCursor.foreground": "#000000",
    "editorGroup.border": "#808080",
    "editorGroup.dropBackground": "#0000801A",
    "editorGutter.background": "#DDDDDD",
    "editorLineNumber.activeForeground": "#000080",
    "editorLineNumber.foreground": "#808080",
    "editorWhitespace.foreground": "#DDDDDD",
    "editorWidget.background": "#DDDDDD",
    "editorWidget.border": "#808080",
    "editorWidget.foreground": "#000000",
    "errorForeground": "#FF0000",
    "focusBorder": "#000080",
    "icon.foreground": "#FFFFFF",
    "input.background": "#FFFFFF",
    "input.border": "#808080",
    "input.foreground": "#000000",
    "input.placeholderForeground": "#808080",
    "inputOption.activeBackground": "#000080",
    "inputOption.activeBorder": "#FFFFFF",
    "inputOption.activeForeground": "#FFFFFF",
    "inputOption.hoverBackground": "#0000801A",
    "list.activeSelectionBackground": "#000080",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.dropBackground": "#0000801A",
    "list.errorForeground": "#FF0000",
    "list.focusBackground": "#000080",
    "list.focusForeground": "#FFFFFF",
    "list.highlightForeground": "#65abf6",
    "list.hoverBackground": "#0000801A",
    "list.inactiveSelectionBackground": "#EEEEEE",
    "list.inactiveSelectionForeground": "#000000",
    "list.warningForeground": "#808000",
    "menu.background": "#DDDDDD",
    "menu.border": "#808080",
    "menu.foreground": "#000000",
    "menu.selectionBackground": "#000080",
    "menu.selectionForeground": "#FFFFFF",
    "menubar.selectionBackground": "#000080",
    "menubar.selectionForeground": "#FFFFFF",
    "panel.background": "#DDDDDD",
    "panel.border": "#808080",
    "quickInput.background": "#DDDDDD",
    "quickInput.foreground": "#000000",
    "quickInputList.focusBackground": "#000080",
    "quickInputList.focusForeground": "#FFFFFF",
    "scrollbar.shadow": "#808080",
    "scrollbarSlider.activeBackground": "#0000807F",
    "scrollbarSlider.background": "#DDDDDD7F",
    "scrollbarSlider.hoverBackground": "#0000804C",
    "sideBar.background": "#DDDDDD",
    "sideBar.border": "#808080",
    "sideBar.foreground": "#000000",
    "sideBarSectionHeader.background": "#000080",
    "sideBarSectionHeader.foreground": "#FFFFFF",
    "statusBar.background": "#DDDDDD",
    "statusBar.border": "#808080",
    "statusBar.foreground": "#000000",
    "statusBar.noFolderBackground": "#DDDDDD",
    "tab.activeBackground": "#E5E5E5",
    "tab.activeBorder": "#FFFFFF",
    "tab.activeForeground": "#000000",
    "tab.border": "#808080",
    "tab.inactiveBackground": "#DDDDDD",
    "tab.inactiveForeground": "#000000",
    "titleBar.activeBackground": "#000080",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.border": "#808080",
    "titleBar.inactiveBackground": "#808080",
    "titleBar.inactiveForeground": "#DDDDDD",
    "toolbar.hoverBackground": "#0000801A",
    "toolbar.hoverOutline": "#000080",
    "window.activeBorder": "#000080",
    "window.inactiveBorder": "#808080"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#800000"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#000080",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "scope": [
        "variable",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#000080",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#800000"
      }
    }
  ]
}