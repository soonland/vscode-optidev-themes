{
  "name": "Windows 98",
  "type": "light",
  "$schema": "vscode://schemas/color-theme",
  "colors": {
    "activityBar.background": "#010081",
    "activityBar.foreground": "#fdffff",
    "activityBarBadge.background": "#ff0081",
    "activityBarBadge.foreground": "#fdffff",
    "activityBarTop.background": "#010081",
    "activityBarTop.foreground": "#fdffff",
    "badge.background": "#010081",
    "badge.foreground": "#fdffff",
    "button.background": "#818181",
    "dropdown.background": "#818181",
    "dropdown.border": "#c3c3c3",
    "dropdown.foreground": "#fdffff",
    "dropdown.listBackground": "#fdffff",
    "editor.background": "#fdffff",
    "editor.findMatchBackground": "#ff0081",
    "editor.findMatchHighlightBackground": "#ff008100",
    "editorCursor.foreground": "#000000",
    "editorGroup.dropBackground": "#818181",
    "editorGutter.background": "#008080",
    "editorLineNumber.activeForeground": "#fdffff",
    "editorLineNumber.foreground": "#c3c3c3",
    "editorWhitespace.foreground": "#c3c3c3",
    "editorWidget.background": "#b2b2b2",
    "editorWidget.foreground": "#000000",
    "errorForeground": "#ff0000",
    "focusBorder": "#010081",
    "input.background": "#fdffff",
    "input.border": "#000000",
    "input.foreground": "#000000",
    "input.placeholderForeground": "#818181",
    "inputOption.activeBackground": "#c3c3c3",
    "inputOption.activeBorder": "#010081",
    "inputOption.activeForeground": "#818181",
    "inputOption.hoverBackground": "#b2b2b2",
    "inputValidation.errorBackground": "#6A041D",
    "inputValidation.errorBorder": "#ff0000",
    "inputValidation.errorForeground": "#fdffff",
    "list.activeSelectionBackground": "#010081",
    "list.activeSelectionForeground": "#fdffff",
    "list.errorForeground": "#ff0081",
    "list.inactiveSelectionBackground": "#818181",
    "list.inactiveSelectionForeground": "#fdffff",
    "list.warningForeground": "#008080",
    "minimap.selectionHighlight": "#ff008100",
    "panel.background": "#e5e5e5",
    "quickInput.background": "#fdffff",
    "quickInputList.focusBackground": "#010081",
    "sideBar.background": "#d4d4d4",
    "sideBarSectionHeader.background": "#010081",
    "sideBarSectionHeader.foreground": "#fdffff",
    "statusBar.background": "#010081",
    "statusBar.noFolderBackground": "#010081",
    "tab.activeBackground": "#010081",
    "tab.activeForeground": "#fdffff",
    "tab.inactiveBackground": "#c3c3c3",
    "tab.inactiveForeground": "#000000",
    "titleBar.activeBackground": "#010081",
    "titleBar.activeForeground": "#fdffff",
    "titleBar.inactiveBackground": "#c3c3c3",
    "titleBar.inactiveForeground": "#000000",
    "toolbar.hoverBackground": "#818181",
    "toolbar.hoverOutline": "#010081"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#3399FF"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#000080"
      }
    }
  ]
}
