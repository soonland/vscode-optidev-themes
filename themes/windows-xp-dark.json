{
  "name": "Windows XP Dark",
  "type": "dark",
  "$schema": "vscode://schemas/color-theme",
  "colors": {
    "activityBar.background": "#3A6EA5",
    "activityBar.foreground": "#fdffff",
    "activityBarBadge.background": "#ff0081",
    "activityBarBadge.foreground": "#fdffff",
    "activityBarTop.background": "#3A6EA5",
    "activityBarTop.foreground": "#fdffff",
    "breadcrumb.background": "#333333",
    "breadcrumb.foreground": "#D4D4D4",
    "button.background": "#39A006",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#4CC417",
    "dropdown.background": "#3A3A3A",
    "dropdown.foreground": "#D4D4D4",
    "editor.background": "#3d3d3d",
    "editor.foreground": "#D4D4D4",
    "editorLineNumber.activeForeground": "#fdffff",
    "editorLineNumber.foreground": "#c3c3c3",
    "editorWhitespace.foreground": "#c3c3c3",
    "input.background": "#252526",
    "input.border": "#B4B4B4",
    "input.foreground": "#D4D4D4",
    "list.activeSelectionBackground": "#3A6EA5",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#1E4D7A",
    "list.inactiveSelectionForeground": "#FFFFFF",
    "panel.background": "#1E1E1E",
    "panel.border": "#333333",
    "sideBar.background": "#333333",
    "sideBar.foreground": "#D4D4D4",
    "statusBar.background": "#3A6EA5",
    "statusBar.foreground": "#FFFFFF",
    "tab.activeBackground": "#3A6EA5",
    "tab.activeForeground": "#fdffff",
    "tab.inactiveBackground": "#c3c3c3",
    "tab.inactiveForeground": "#000000",
    "terminal.background": "#1E1E1E",
    "terminal.foreground": "#D4D4D4",
    "titleBar.activeBackground": "#3A6EA5",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#1E4D7A",
    "titleBar.inactiveForeground": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#ffae00"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#19a700"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#007bff"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#19a700"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#007bff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#b3b3b3"
      }
    }
  ]
}
