    "activityBar.background": "#000080",
    "sideBar.background": "#C0C0C0",
    "sideBar.foreground": "#000000",
    "statusBar.background": "#000080",
    "statusBar.foreground": "#FFFFFF",
    "titleBar.activeBackground": "#000080",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#808080",
    "titleBar.inactiveForeground": "#FFFFFF",
    "button.background": "#C0C0C0",
    "button.foreground": "#000000",
    "button.hoverBackground": "#00FFFF",
    "dropdown.background": "#C0C0C0",
    "dropdown.foreground": "#000000",
    "input.background": "#FFFFFF",
    "input.foreground": "#000000",
    "input.border": "#808080",
    "list.activeSelectionBackground": "#00FFFF",
    "list.activeSelectionForeground": "#000000",
    "list.inactiveSelectionBackground": "#808080",
    "list.inactiveSelectionForeground": "#000000",
    "panel.background": "#C0C0C0",
    "panel.border": "#808080",
    "terminal.background": "#C0C0C0",
    "terminal.foreground": "#000000"
